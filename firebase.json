{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/",
        "destination": "/zh-TW/index.html"
      },
      {
        "source": "/zh-TW",
        "destination": "/zh-TW/index.html"
      },
      {
        "source": "/en",
        "destination": "/en/index.html"
      },
      {
        "source": "/ja",
        "destination": "/ja/index.html"
      },
      {
        "source": "/fr",
        "destination": "/fr/index.html"
      },
      {
        "source": "/de",
        "destination": "/de/index.html"
      },
      {
        "source": "/es",
        "destination": "/es/index.html"
      },
      {
        "source": "/pt",
        "destination": "/pt/index.html"
      }
    ]
  }
}
